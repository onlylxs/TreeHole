.FollowBtn {
    position: fixed;
    top: 24rpx;
    right: 240rpx;
    z-index: 100000;
    height: 40rpx;
    line-height: 40rpx;
    font-size: 26rpx;
    border: 1rpx solid #ed711a;
    padding: 0 14rpx;
    border-radius: 4rpx;
}

.FollowBtn.selected {
    background: #ed711a;
    color: #fff;
}

